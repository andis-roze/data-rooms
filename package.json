{
  "name": "home_assignment",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage --testTimeout=15000",
    "i18n:extract:keys": "i18next \"src/**/*.{ts,tsx}\" --config i18next-parser.config.cjs",
    "i18n:extract": "npm run i18n:extract:keys && node scripts/i18n/extract-gettext.mjs",
    "i18n:gettext:compile": "node scripts/i18n/compile-gettext.mjs",
    "i18n:sync": "npm run i18n:extract && npm run i18n:gettext:compile"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/manrope": "^5.2.8",
    "@mui/material": "^7.3.8",
    "i18next": "^25.8.10",
    "i18next-browser-languagedetector": "^8.2.1",
    "i18next-icu": "^2.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gettext-converter": "^1.3.1",
    "globals": "^16.5.0",
    "i18next-parser": "^9.0.2",
    "jsdom": "^28.1.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
