{
  "name": "data_rooms",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:gh-pages": "node scripts/build-gh-pages.mjs",
    "deploy:gh-pages": "node scripts/deploy-gh-pages.mjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:smoke:home": "vitest run src/test/app/App.test.tsx -t \"renders home route with data room shell and localized navigation|creates, renames, and deletes a folder from row actions|selects newly created folder and uploaded file without navigating into them|paginates list view and allows changing items per page|moves selected folders and files to another folder\"",
    "audit:unused:types": "tsc --noEmit --noUnusedLocals --noUnusedParameters",
    "audit:unused:exports": "npx --yes ts-prune",
    "audit:unused:deps": "npx --yes depcheck --ignores=@types/node,@types/react,@types/react-dom,@vitest/coverage-v8,gh-pages",
    "audit:unused": "npm run audit:unused:types && npm run audit:unused:exports && npm run audit:unused:deps",
    "test:coverage": "vitest run --coverage --testTimeout=15000",
    "record:ux": "node scripts/record-ux-walkthrough.mjs",
    "record:ux:slow": "node scripts/record-ux-walkthrough.mjs --step-delay-ms=3000 --action-delay-ms=700",
    "record:ux:slower": "node scripts/record-ux-walkthrough.mjs --step-delay-ms=4200 --action-delay-ms=1000",
    "record:ux:install-browser": "playwright install chromium",
    "prepare": "node scripts/install-git-hooks.mjs",
    "hooks:install": "node scripts/install-git-hooks.mjs",
    "hooks:pre-push": "node scripts/verify-pre-push.mjs",
    "i18n:extract:keys": "i18next \"src/**/*.{ts,tsx}\" --config i18next-parser.config.cjs",
    "i18n:prune-unused": "node scripts/i18n/prune-unused.mjs",
    "i18n:extract": "npm run i18n:extract:keys && npm run i18n:prune-unused && node scripts/i18n/extract-gettext.mjs",
    "i18n:gettext:compile": "node scripts/i18n/compile-gettext.mjs",
    "i18n:sync": "npm run i18n:extract && npm run i18n:gettext:compile",
    "i18n:verify-sync": "node scripts/i18n/verify-sync.mjs",
    "i18n:check": "npm run i18n:sync && npm run i18n:verify-sync && git diff --exit-code -- gettext src/i18n/locales"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/manrope": "^5.2.8",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "fflate": "^0.8.2",
    "i18next": "^25.8.10",
    "i18next-browser-languagedetector": "^8.2.1",
    "i18next-icu": "^2.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gettext-converter": "^1.3.1",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "i18next-parser": "^9.0.2",
    "jsdom": "^28.1.0",
    "playwright": "^1.58.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
