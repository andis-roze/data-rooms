VITE_DEV_HOST=0.0.0.0
VITE_DEV_PORT=5173

# Set these when browser HMR websocket cannot auto-detect host/ports
# (for example Docker, VM, reverse proxy, or remote dev).
# VITE_HMR_HOST=localhost
# VITE_HMR_PORT=5173
# VITE_HMR_CLIENT_PORT=5173

# Enable polling file watch only when native FS events do not propagate.
# VITE_HMR_POLLING=true
# VITE_HMR_POLL_INTERVAL=250

# Auto-dismiss timeout for bottom-right feedback messages in milliseconds.
# Set to 0 to disable auto-dismiss.
# VITE_FEEDBACK_TIMEOUT_MS=6000
